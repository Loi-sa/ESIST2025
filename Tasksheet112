/* Create all of the primitives (except long and double) with different values. 
Concatenate them into a string and  print it to the screen 
so it will print: H3110 w0r1d 2.0 true
*/
class Tasksheet1112 {
        byte zero = 0;
        String output ="W"+zero+"w";
        System.out.println(output);
 
        String str1 = "H311O";
        String str2 = "wOr1d";
        float f = 2.0f;
        boolean t = true;

        System.out.println(str1+" "+str2+" "+f+" "+t);
}
